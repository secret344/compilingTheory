{
    "code-runner.executorMap": {
        "c": "cd $dir && cd emsdk && emsdk activate latest && cd .. && cd build && emcmake cmake ..  && emmake make && node lexical.js"
    },
    "files.associations": {
        "*.cjson": "jsonc",
        "*.wxss": "css",
        "*.wxs": "javascript",
        "*.wxml": "wxml",
        "*.vue": "vue",
        "*.dart": "dart",
        "nfa_intepretor.h": "c",
        "random": "c",
        "valarray": "c",
        "regex": "c",
        "sstream": "c",
        "strstream": "c"
    }
}
